<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TwitterFilterService WSDL service -->

  <service name="TwitterFilter" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://twitter.web_services.data_mashup.as.ipvs.uni_stuttgart.de"/>
      <parameter name="wsdlServiceElement" value="TwitterFilterService"/>
      <parameter name="schemaQualified" value="http://twitter.web_services.data_mashup.as.ipvs.uni_stuttgart.de"/>
      <parameter name="wsdlServicePort" value="TwitterFilter"/>
      <parameter name="className" value="de.uni_stuttgart.ipvs.as.data_mashup.web_services.twitter.TwitterFilterSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TwitterFilter"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="filterData" qname="operNS:filterData" xmlns:operNS="http://twitter.web_services.data_mashup.as.ipvs.uni_stuttgart.de" returnQName="retNS:filterDataReturn" xmlns:retNS="http://twitter.web_services.data_mashup.as.ipvs.uni_stuttgart.de" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:key" xmlns:pns="http://twitter.web_services.data_mashup.as.ipvs.uni_stuttgart.de" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:criteria" xmlns:pns="http://twitter.web_services.data_mashup.as.ipvs.uni_stuttgart.de" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="filterData"/>

  </service>
</deployment>
